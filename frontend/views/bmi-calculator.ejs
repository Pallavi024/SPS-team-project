<%- include ("./partials/header")%>

<!--bmi calculator start-->


<div class="bmibody">
    <div class="calc-box">
        <h2 class="bmi-heading">BMI Calculator</h2>
        <p class="text">Height</p>
        <input type="text" id="height">
        <p class="text">Weight</p>
        <input type="text" id="weight">
        <p id="result"></p>
        <p id="status"></p>
        <p id="suggestion"></p>
        <button id="calculate-btn">Calculate</button>
        <p id="enter-details-info">Please enter height [cm] and weight [kg]</p>
    </div>
</div>

<!--bmi calculator end-->
















